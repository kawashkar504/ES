#include<AT89S52.h>
#define LED P2_4
#define ON 0	q	                                                                                                                                                                                                                  
#define OFF 1
#define SW P3_3
#define INPUT 1
void main()
{
__bit status;  /* initialize the bit variable*/
LED=OFF;
SW=INPUT;
while(1)
{
status=SW;
LED=!status;
}
}
